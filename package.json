{
  "name": "cloudflare-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "denon start",
    "start": "deno run --allow-env --allow-read --allow-write --allow-net ./src/index.ts",
    "test": "deno test --allow-all",
    "format": "prettier --write ./src",
    "prebuild": "rm -rf ./execute",
    "build": "deno compile -c ./tsconfig.json --allow-env --allow-read --allow-write --allow-net --output=execute ./src/index.ts",
    "deno-test-csv": "deno run --allow-env --allow-read --allow-write --unstable ./src/helpers/csv.test.ts"
  }
}
